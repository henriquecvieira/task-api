    "@nestjs/swagger": "^6.3.0",
    "ajv": "^8.12.0",
    "amqplib": "^0.10.3",
    "axios": "^1.3.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^7.8.8",
    "nodemailer": "^6.9.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^9.0.0",
    "validator": "^13.15.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
